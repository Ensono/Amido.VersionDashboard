@model Amido.VersionDashboard.Web.Models.Dashboard
@{
    ViewBag.Title = Model.Title;
}

@foreach (var dashboardSection in Model.Sections) {
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <h2 class="page-header">@dashboardSection.Title</h2>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        @foreach (var panel in dashboardSection.Panels) {
            @Html.Partial("Version", panel)
        }
    </div>
    <!-- /.row -->
}

<script src="@Url.Content("~/bower_components/morrisjs/morris.min.js")"></script>